{
  "name": "@form-atoms/field",
  "packageManager": "yarn@4.0.2",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/field#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/field.git"
  },
  "bugs": "https://github.com/form-atoms/field/issues",
  "license": "MIT",
  "keywords": [
    "react atom field",
    "react field",
    "react input",
    "form atoms",
    "form atoms field",
    "form fields",
    "jotai",
    "jotai form",
    "jotai form fields",
    "react",
    "react form",
    "react hook field"
  ],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json --force",
    "build:docs": "storybook build --docs",
    "build:storybook": "storybook build",
    "test": "vitest",
    "typecheck": "vitest typecheck",
    "storybook": "storybook dev",
    "sb": "yarn storybook",
    "docs": "storybook dev --docs",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "semantic-release": "semantic-release",
    "ui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@mdx-js/react": "^2.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "7.6.0",
    "@storybook/addon-docs": "7.6.0",
    "@storybook/addon-essentials": "7.6.0",
    "@storybook/addon-interactions": "7.6.0",
    "@storybook/addon-links": "7.6.0",
    "@storybook/builder-vite": "^7.6.0",
    "@storybook/react": "7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/eslint": "^8.44.7",
    "@types/lodash.shuffle": "^4.2.9",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "form-atoms": "^3.2.0",
    "happy-dom": "^12.10.3",
    "jotai": "2.6.0",
    "jotai-devtools": "0.7.0",
    "jotai-effect": "0.2.3",
    "lodash.shuffle": "^4.2.0",
    "prettier": "3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^22.0.8",
    "storybook": "7.6.0",
    "typescript": "5.3.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "zod": "3.22.4"
  },
  "dependencies": {
    "react-render-prop-type": "0.1.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "form-atoms": "^3",
    "jotai": "^2",
    "jotai-effect": "^0",
    "react": ">=16.8",
    "zod": "^3"
  }
}
