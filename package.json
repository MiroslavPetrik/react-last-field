{
  "name": "@form-atoms/field",
  "packageManager": "yarn@4.0.2",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/MiroslavPetrik/form-atoms-field#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/MiroslavPetrik/form-atoms-field.git"
  },
  "bugs": "https://github.com/MiroslavPetrik/form-atoms-field/issues",
  "license": "MIT",
  "keywords": [
    "react last field",
    "react atom field",
    "react field",
    "react input",
    "form atoms",
    "form atoms field",
    "form fields",
    "jotai",
    "jotai form",
    "jotai form fields",
    "react",
    "react form",
    "react hook field"
  ],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json --force",
    "build:docs": "storybook build --docs",
    "build:storybook": "storybook build",
    "test": "vitest",
    "typecheck": "vitest typecheck",
    "storybook": "storybook dev",
    "sb": "yarn storybook",
    "docs": "storybook dev --docs",
    "lint": "eslint --ext .tsx ./packages",
    "lint:fix": "yarn run lint --fix",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@emotion/react": "^11.11.1",
    "@mdx-js/react": "^1.6.22",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "7.6.0",
    "@storybook/addon-docs": "7.6.0",
    "@storybook/addon-essentials": "7.6.0",
    "@storybook/addon-interactions": "7.6.0",
    "@storybook/addon-links": "7.6.0",
    "@storybook/builder-vite": "^7.6.0",
    "@storybook/react": "7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8",
    "@types/jsdom": "^21",
    "@types/lodash.shuffle": "^4",
    "@types/prettier": "^2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semantic-release": "^20",
    "@types/testing-library__jest-dom": "^5",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "form-atoms": "^3.0.2",
    "happy-dom": "^8.9.0",
    "jotai": "2.4.3",
    "jotai-devtools": "0.7.0",
    "jotai-effect": "0.1.0",
    "jsdom": "^21.1.0",
    "lodash.shuffle": "^4.2.0",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^20.1.0",
    "storybook": "7.6.0",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.32.0",
    "zod": "3.21.4"
  },
  "dependencies": {
    "react-render-prop-type": "0.1.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "form-atoms": "^3",
    "jotai": "^2",
    "jotai-effect": "^0",
    "react": ">=16.8",
    "zod": "^3"
  }
}
